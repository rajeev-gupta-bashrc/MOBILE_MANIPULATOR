<?xml version="1.0"?>
<!-- manipulator Chain -->
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="arm">

<!-- supplementary links -->
  <xacro:macro name="Supp_Link" params="link_number length">
    <link name="supp_link${link_number}">
      <visual>
        <origin xyz="0 0 0" rpy="-1.57 0 0"/>
        <geometry>
          <cylinder radius="0.04" length="0.04"/>
        </geometry>
      </visual>
    </link>
    <joint name="supp_joint${link_number}" type="fixed">
      <parent link="link${link_number}"/>
      <child link="supp_link${link_number}"/>
      <origin xyz="0.0 0.0 ${length}" rpy="0 0 0"/>
    </joint>
  </xacro:macro>

  <!-- <xacro:arg name="d1" default="0.147"/>
  <xacro:arg name="a1" default="0.155"/>
  <xacro:arg name="a2" default="0.135"/>
  <xacro:arg name="d2" default="0.2175"/> -->
  
  <!-- World -->
  <!-- <link name="world"/> -->

  <!-- World fixed joint-->
  <!-- <joint name="world_fixed" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="world"/>
    <child link="arm_base"/>
  </joint> -->
  <!-- BaseLink  -->
  <link name="arm_base">
    <visual>
      <origin xyz="0 0 0.01" rpy="0 0 0"/>
      <geometry>
      <box size="0.08 0.08 0.02"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="3.0876154e-04 0.0000000e+00 -1.2176461e-04" />
      <mass value="2.0" />
       <inertia ixx="0.001393333333" ixy="0.0" ixz="0.0"
           iyy="0.001393333333" iyz="0.0"
           izz="0.000277333333"/>
    </inertial>
  </link>

  <!-- Joint 1 -->
  <joint name="joint1" type="revolute">
    <parent link="arm_base"/>
    <child link="link1"/>
    <origin xyz="0.0 0.0 0.02" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="4.8" effort="1" lower="${-pi*169/180}" upper="${pi*169/180}" />
  </joint>

  <!--  Link 1 -->
  <link name="link1">
    <visual>
      <origin xyz="0 0 ${0.147/2}" rpy="0 0 0"/>
      <geometry>
        <box size="0.03 0.03 ${0.147}"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="-3.0184870e-04 5.4043684e-04 ${0.018 + 2.9433464e-02}" />
      <mass value="9.8406837e-02" />
      <inertia ixx="3.4543422e-05" ixy="-1.6031095e-08" ixz="-3.8375155e-07"
               iyy="3.2689329e-05" iyz="2.8511935e-08"
               izz="1.8850320e-05" />
    </inertial>
  </link>

  <xacro:Supp_Link link_number="1" length="0.147"/>

  <!--  Joint 2 -->
  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0.0 0.0 ${0.147}" rpy="0 ${pi/2} 0"/>
    <axis xyz="0 -1 0"/>
    <limit velocity="4.8" effort="2" lower="${-pi*65/180}" upper="${pi*90/180}" />
  </joint>

  <!--  Link 2 -->
  <link name="link2">
    <visual>
      <origin xyz="0 0 ${0.155/2}" rpy="0 0 0"/>
      <geometry>
      <box size="0.03 0.03 ${0.155}"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="1.0308393e-02 3.7743363e-04 1.0170197e-01" />
      <mass value="1.3850917e-01" />
      <inertia ixx="3.3055381e-04" ixy="-9.7940978e-08" ixz="-3.8505711e-05"
               iyy="3.4290447e-04" iyz="-1.5717516e-06"
               izz="6.0346498e-05" />
    </inertial>
  </link>

  <xacro:Supp_Link link_number="2" length="0.155"/>

  <!--  Joint 3 -->
  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 ${0.155}" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit velocity="4.8" effort="1" lower="${-pi*150/180}" upper="${pi*146/180}" />
  </joint>

  <!--  Link 3 -->
  <link name="link3">
    <visual>
      <origin xyz="0 0 ${0.135/2}" rpy="0 0 0"/>
      <geometry>
        <box size="0.03 0.03 ${0.135}"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="9.0909590e-02 3.8929816e-04 2.2413279e-04" />
      <mass value="1.3274562e-01" />
      <inertia ixx="3.0654178e-05" ixy="-1.2764155e-06" ixz="-2.6874417e-07"
               iyy="2.4230292e-04" iyz="1.1559550e-08"
               izz="2.5155057e-04" />
    </inertial>
  </link>

  <xacro:Supp_Link link_number="3" length="0.135"/>


  <!--  Joint 4 -->
  <joint name="joint4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0.0 0.0 ${0.135}" rpy="0 ${pi/2} 0"/>
    <axis xyz="0 -1 0"/>
    <limit velocity="4.8" effort="1" lower="${-pi*102.5/180}" upper="${pi*102.5/180}" />
  </joint>

  <!--  Link 4 -->
  <link name="link4">
    <visual>
      <origin xyz="0 0 ${0.2175/2}" rpy="0 0 0"/>
      <geometry>
        <box size="0.03 0.03 ${0.2175}"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <inertial>
      <origin xyz="4.4206755e-02 3.6839985e-07 8.9142216e-03" />
      <mass value="1.4327573e-01" />
      <inertia ixx="8.0870749e-05" ixy="0.0" ixz="-1.0157896e-06"
               iyy="7.5980465e-05" iyz="0.0"
               izz="9.3127351e-05" />
    </inertial>
  </link>

  <xacro:Supp_Link link_number="4" length="0.2175"/>

  <!--  Joint5 -->
  <joint name="joint5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0 0 ${0.2175}" rpy="0 -${pi/2} 0"/>
    <axis xyz="1 0 0"/>
    <limit velocity="4.8" effort="1" lower="${-pi*167/180}" upper="${pi*167/180}" />
  </joint>

  <!--  Link 5 -->
  <link name="link5">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator/meshes/chain_link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <!-- <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator/meshes/chain_link5.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision> -->

    <inertial>
      <origin xyz="4.4206755e-02 3.6839985e-07 8.9142216e-03" />
      <mass value="1.4327573e-01" />
      <inertia ixx="8.0870749e-05" ixy="0.0" ixz="-1.0157896e-06"
               iyy="7.5980465e-05" iyz="0.0"
               izz="9.3127351e-05" />
    </inertial>
  </link>

  <!--  joint_l -->
  <joint name="joint_gripper_l" type="prismatic">
    <parent link="link5"/>
    <child link="gripper_l"/>
    <origin xyz="0.0817 0.021 0.0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit velocity="4.8" effort="1" lower="0" upper="0" />
  </joint>

  <!--  gripper_l -->
  <link name="gripper_l">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator/meshes/chain_link_grip_l.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <!-- <collision>
      <origin xyz="0.0 0.0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator/meshes/chain_link_grip_l.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision> -->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1.0e-03" />
      <inertia ixx="1.0e-03" ixy="0.0" ixz="0.0"
               iyy="1.0e-03" iyz="0.0"
               izz="1.0e-03" />
    </inertial>
  </link>

  <!--  joint_r -->
  <joint name="joint_gripper_r" type="prismatic">
    <parent link="link5"/>
    <child link="gripper_r"/>
    <origin xyz="0.0817 -0.021 0" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit velocity="4.8" effort="1" lower="0" upper="0" />
    <mimic joint="joint_gripper_l" multiplier="1"/>
  </joint>

  <!--  gripper_r -->
  <link name="gripper_r">
    <visual>
      <origin xyz="0.0 -0.0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator/meshes/chain_link_grip_r.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>

    <!-- <collision>
      <origin xyz="0.0 -0.0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://manipulator/meshes/chain_link_grip_r.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision> -->

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="1.0e-03" />
      <inertia ixx="1.0e-03" ixy="0.0" ixz="0.0"
               iyy="1.0e-03" iyz="0.0"
               izz="1.0e-03" />
    </inertial>
  </link>

</robot>
